doctype html5
html(lang='utf-8')
    head
        title bigbrother /
    body
        h1 bigbrother /

        div.exists-beacon
            h3 current beacons
            ul.beacon
                - for (var i=0;i<beacons.length;++i) {
                    li
                        a(href!="/beacon/#{beacons[i].uuid}/html")
                            span #{beacons[i].uuid} : 
                            span #{beacons[i].name}
                -}
